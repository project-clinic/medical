
<a href="/doctor/new">New Doctor</a>
<div class="container">
  <h2><%= title %></h2>
  <table>
    <thead>
      <tr>
        <th>NAME</th>
        <th>SPECIALITY</th>
        <th>MEDICAL IDENTITY</th>
        <th></th>
        <th></th>

      </tr>
    </thead>
    <% doctors.forEach(user => { %>
    <tbody>

        <td><%= user.name %><span> <%= user.surname %></span></td>
        <td><%= user.professional.speciality %></td>
        <td><%= user.professional.collegiate %></td>

        <td>
        <button data-target="edit-doctor" class="btn modal-trigger">Edit</button>
          <%- include('./edit-doctor', {user})%>
        <td>
        <td>
          <form action="/<%= user._id %>/delete" method="get">
            <button data-target="delete-doctor" class="btn modal-trigger red">Delete</button>
          </form>
        </td>
    </tbody>
    <%  }) %>
  </div>
</table>
