<ul class="collapsible" data-collapsible="expandable">
  <% pathos.forEach(patho => { %>
    <li>
      <div class="collapsible-header">
        <%= patho.name %>
        <a class="waves-effect waves-light btn modal-trigger" href="#modal2">New Report</a>
      </div>
      <div id="modal2" class="modal">
        <%- include('../report/new-report', { patho, patient }) %>
      </div>
      <div class="collapsible-body">
        <%- include('../report/report-list', { reports, patho }) %>
      </div>
    </li>
  <%  }) %>
</ul>