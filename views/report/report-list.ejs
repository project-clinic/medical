<% reports.forEach(report => { %>
  <% if(report.pathologyId.toString() == patho._id.toString()) { %>
    <div class="section row">
      <div class="row">
        <div class="col s1"></div>
        <div class="col s11">
          <ul class="tabs">
            <li class="tab col s1"></li>
            <li class="tab col s2"><a class="active" href="#<%= report._id %>1">Symptoms</a></li>
            <li class="tab col s2"><a href="#<%= report._id %>2">Consulty Work</a></li>
            <li class="tab col s2"><a href="#<%= report._id %>3">Treatment</a></li>
          </ul>
        </div>
        <div class="col s4"></div>
      </div>
      <div class="row">
        <div class="col s2">
          <h6><b><%= report.updatedAt.toDateString() %></b></h6>
        </div>
        <div id="<%= report._id %>1" class="col s7">
          <span class="report-<%= patho._id %>"><%= report.symptoms %></span>
        </div>
        <div id="<%= report._id %>2" class="col s7">
          <span class="report-<%= patho._id %>"><%= report.consultyWork %></span>
        </div>
        <div id="<%= report._id %>3" class="col s7">
          <span class="report-<%= patho._id %>"><%= report.treatment %></span>
        </div>
        <div class="col s3">
          <span>Images</span>
        </div>
      </div>
    </div>
    <div class="divider"></div>
  <% } %>
<% }) %>